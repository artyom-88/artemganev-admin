{
  "author": "Artem Ganev",
  "homepage": "https://artyom-88.github.io",
  "license": "GPL-3.0",
  "name": "artemganev-admin",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artyom-88/artemganev-admin.git"
  },
  "engines": {
    "node": "18.x",
    "pnpm": "8.x"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ],
  "scripts": {
    "build": "next build",
    "depcheck": "pnpm dlx depcheck",
    "lint": "next lint --max-warnings 1 --quiet",
    "lint:fix": "next lint --fix",
    "lint:warn": "next lint",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "start:dev": "next dev -p 8080",
    "start:local": "export PORT=8080 && pnpm start:prod",
    "start:prod": "next start -p $PORT",
    "test": "echo 'Tests are not implemented yet'",
    "update": "pnpm dlx npm-check -u -E -s"
  },
  "dependencies": {
    "@nextui-org/react": "2.2.9",
    "@nextui-org/system": "2.0.15",
    "@nextui-org/theme": "2.1.17",
    "@tanstack/react-query": "5.17.0",
    "dayjs": "1.11.10",
    "formik": "2.4.5",
    "framer-motion": "10.16.16",
    "ky": "1.1.3",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-datepicker": "4.25.0",
    "react-dom": "18.2.0",
    "yup": "1.3.3",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "5.14.6",
    "@types/node": "20.11.16",
    "@types/react": "18.2.46",
    "@types/react-datepicker": "4.19.5",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.2",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "sass": "1.69.5",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3"
  },
  "pnpm": {
    "overrides": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
